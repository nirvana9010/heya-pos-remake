# Example .env configuration for upgraded Supabase plan

# Database URLs from Supabase dashboard
DATABASE_URL=postgresql://[user]:[password]@[project].pooler.supabase.com:6543/postgres?pgbouncer=true&connection_limit=10&sslmode=require
DIRECT_URL=postgresql://[user]:[password]@[project].supabase.co:5432/postgres

# Connection pool settings for upgraded plan
DATABASE_CONNECTION_LIMIT=10
DATABASE_POOL_TIMEOUT=10
DATABASE_IDLE_TIMEOUT=10
DATABASE_STATEMENT_TIMEOUT=30000

# Other required variables
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=7d
SUPABASE_URL=https://[project].supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
